    "@types/md5": "^2.3.1",
    "@types/minimist": "^1.2.2",
    "ava": "^3.15.0",
    "pkg": "^4.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "commander": "^5.0.0",
    "fs-extra": "^10.0.0",
    "is-ci": "^3.0.0",
    "md5": "^2.3.0",
    "minimist": "^1.2.5"
  }
